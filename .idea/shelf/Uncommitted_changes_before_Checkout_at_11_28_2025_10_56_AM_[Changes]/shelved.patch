+:root {
+    --ices-navy: #1f1a50;
+    --ices-orange: #fe7701;
+}
+
+body {
+    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif;
+    margin: 0;
+    padding: 0;
+}
+
+/* Typography defaults */
+h1 {
+    font-size: 2.25rem;
+    font-weight: 700;
+    line-height: 1.2;
+}
+
+h2 {
+    font-size: 1.875rem;
+    font-weight: 700;
+    line-height: 1.3;
+}
+
+h3 {
+    font-size: 1.25rem;
+    font-weight: 600;
+    line-height: 1.4;
+}
+
+h4 {
+    font-size: 1.125rem;
+    font-weight: 600;
+    line-height: 1.4;
+}
+
+/* Header styles */
+.header-nav a {
+    transition: color 0.2s;
+}
+
+.header-nav a:hover {
+    color: var(--ices-orange);
+}
+
+/* Mobile menu */
+.mobile-menu {
+    display: none;
+    background-color: var(--ices-navy);
+    padding: 1rem;
+}
+
+.mobile-menu.active {
+    display: block;
+}
+
+.mobile-menu a {
+    display: block;
+    padding: 0.75rem 0;
+    color: white;
+    border-bottom: 1px solid rgba(255, 255, 255, 0.1);
+    transition: color 0.2s;
+}
+
+.mobile-menu a:hover,
+.mobile-menu a.active {
+    color: var(--ices-orange);
+}
+
+/* Executive card styles - Modern flat design */
+.exec-card {
+    position: relative;
+    overflow: hidden;
+    transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
+}
+
+.exec-card::before {
+    content: '';
+    position: absolute;
+    top: 0;
+    left: 0;
+    right: 0;
+    height: 4px;
+    background: linear-gradient(90deg, var(--ices-orange), #ffa500);
+    transform: scaleX(0);
+    transition: transform 0.4s ease;
+}
+
+.exec-card:hover::before {
+    transform: scaleX(1);
+}
+
+.exec-card:hover {
+    transform: translateY(-4px);
+    box-shadow: 0 12px 24px rgba(31, 26, 80, 0.15);
+}
+
+.exec-photo {
+    width: 100%;
+    height: 280px;
+    object-fit: cover;
+    filter: grayscale(30%);
+    transition: all 0.4s ease;
+}
+
+.exec-card:hover .exec-photo {
+    filter: grayscale(0%);
+    transform: scale(1.03);
+}
+
+.exec-content {
+    position: relative;
+    background: white;
+}
+
+.position-badge {
+    display: inline-block;
+    padding: 0.375rem 0.875rem;
+    border-radius: 20px;
+    font-size: 0.75rem;
+    font-weight: 600;
+    letter-spacing: 0.5px;
+    text-transform: uppercase;
+    background: linear-gradient(135deg, var(--ices-navy), #2d2670);
+    color: white;
+}
+
+.leadership-badge {
+    background: linear-gradient(135deg, var(--ices-orange), #ff8c00);
+}
+
+/* Photo overlay on hover */
